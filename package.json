{
  "name": "stanza-publishoptions",
  "version": "1.0.2",
  "description": "Stanzajs plugin for publishing with options",
  "main": "./dist/index.min.js",
  "types": "dist",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "!/dist/**/*.js.map",
    "!/dist/**/*.ts.map",
    "!/dist/**/index.js"
  ],
  "keywords": [
    "XMPP",
    "stanza",
    "plugin",
    "pubsub"
  ],
  "scripts": {
    "build": "tsc && npm run minify",
    "test": "echo \"Error: no test specified\" && exit 1",
    "minify": "uglifyjs --compress --mangle --output ./dist/index.min.js -- ./dist/index.js",
    "lint": "eslint . --ext .ts"
  },
  "author": "Chris Myers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmyers/stanza-publishoptions.git"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "typescript": "^4.2.3",
    "uglify-js": "^3.13.3"
  },
  "dependencies": {
    "stanza": "^12.15.0"
  }
}
